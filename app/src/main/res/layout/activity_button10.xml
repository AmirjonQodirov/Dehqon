                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/btn10_input_5"
                android:layout_marginLeft="25dp"
                android:layout_marginTop="20dp"
                android:layout_marginRight="25dp"
                android:layout_marginBottom="10dp"
                android:text="@string/btn_inp_6_t8"
                android:textColor="@color/black"
                android:textSize="18sp" />

            <EditText
                android:id="@+id/btn10_input_6"
                android:layout_width="match_parent"
                android:layout_height="40dp"
                android:layout_below="@+id/inp_6"
                android:layout_marginLeft="25dp"
                android:layout_marginRight="25dp"
                android:background="@drawable/inputs"
                android:hint="@string/input_tint"
                android:inputType="text"
                android:paddingLeft="20dp"
                android:textColorHint="@color/gray" />

            <Button
                android:layout_width="match_parent"
                android:layout_height="40dp"
                android:layout_below="@id/btn10_input_6"
                android:layout_marginLeft="30dp"
                android:layout_marginTop="45dp"
                android:layout_marginRight="30dp"
                android:layout_marginBottom="45dp"
                android:background="@drawable/my_button"
                android:onClick="fill_btn_10"
                android:text="@string/button_dialog"
                android:textSize="15sp" />
        </RelativeLayout>
    </ScrollView>
</RelativeLayout>

